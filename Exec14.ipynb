   "source": [
    "题目：将一个正整数分解质因数。例如：输入90,打印出90=2*3*3*5。"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 29,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "2*2*5*5\n"
     ]
    }
   ],
   "source": [
    "def numsplit(i,k):\n",
    "    for j in range(2,i+1):\n",
    "        m = int(i/j)\n",
    "        if i%j == 0:\n",
    "            numsplit(m,k)\n",
    "            k.append(str(j))\n",
    "            break\n",
    "k=[]\n",
    "numsplit(100,k)\n",
    "print(\"*\".join(sorted(k)))\n"
   ]
  }
 ],
